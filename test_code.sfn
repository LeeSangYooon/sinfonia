class Person {
    int age;
    str name;
    
    # int, str -> none
    init age name {
        this.age = age;
        this.name = name;
    }

    # none -> none
    introduce {
        print this.name;
        print this.age;
    }
}

# Person -> int
get_age person {
    return person.age + 1;
}

let Person sangyun = Person "Sangyun" 18;
let int age = (get_age sangyun) * 2 / 3 + 1;
print age;

